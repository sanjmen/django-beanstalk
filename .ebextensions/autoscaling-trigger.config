option_settings:
  aws:autoscaling:trigger:
    MeasureName: CPUUtilization
    Statistic: Average
    Unit: Percent
    Period: '15'
    BreachDuration: '30'
    EvaluationPeriods: '2'
    LowerThreshold: '5'
    UpperThreshold: '90'
    LowerBreachScaleIncrement: '-1'
    UpperBreachScaleIncrement: '1'